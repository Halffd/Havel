print("2. Logical operators:");
let b = false;
print("b =", b);
let ab = a && b;
print("a && b =", ab);
let ab2 = a || b;
print("a || b =", ab2);
let nb = !b;
print("!b =", nb);

// Test 3: Module function overriding via assignment
print("3. Module function overriding via assignment:");
brightnessManager.increaseBrightness = (br) => { 
    print("Custom brightness increase: " + br);
    return "increased by " + br;
}
print(brightnessManager.increaseBrightness("10%"));

// Test 4: Direct module function override
print("4. Direct module function override:");
function brightnessManager.increaseBrightness(amount) {
    return "Direct override: " + amount;
}
print(brightnessManager.increaseBrightness("20%"));

