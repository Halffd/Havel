print("=== Testing FileManager Module ===");

// Test filemanager module
print("filemanager module:", filemanager != null ? "WORKS" : "FAILS");

// Test File object creation
if (filemanager) {
    print("Creating File object...");
    file = filemanager.File("/tmp/test_havel.txt");
    print("File object created:", file != null ? "SUCCESS" : "FAILED");
    
    if (file) {
        print("File path:", file.path);
        
        // Test file operations
        print("Writing to file...");
        file.write("Hello from Havel FileManager!");
        
        print("Reading from file...");
        content = file.read();
        print("File content:", content);
        
        print("File exists:", file.exists());
        print("File size:", file.size());
    }
}

